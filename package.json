{
  "name": "neo4j-browser",
  "version": "1.0.0",
  "description": "Enjoy your data",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --inline --content-base ./dist --history-api-fallback",
    "lint": "standard --fix",
    "test": "npm run lint && jest",
    "dev": "jest --watch"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|html)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "neo4j": "<rootDir>/__mocks__/neo4j.js"
    }
  },
  "standard": {
    "ignore": [
      "dist",
      "src/visualisation/external"
    ],
    "globals": [
      "it",
      "describe",
      "beforeEach",
      "afterEach",
      "neo",
      "FileReader",
      "Blob"
    ]
  },
  "repository": {},
  "keywords": [],
  "author": "",
  "license": "GPL",
  "homepage": "https://neo4j.com",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
